#No.1 & 2
spam = []
for i in range(10):
    print('Please input a number.')
    spam.append(input('number='))
print('Thanks! The list you input is: ' + str(spam))

myList = []
for j in spam:
    if int(j) < 5:
        myList.append(int(j))
    else:
        continue
    
print(myList)

#No.3

print('Please input a number again.')
biasNumber = int(input())
newList = []
for s in spam:
    if int(s) < biasNumber:
        newList.append(int(s))
    else:
        continue

print(newList)



